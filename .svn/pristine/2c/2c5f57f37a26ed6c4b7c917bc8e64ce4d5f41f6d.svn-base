using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.UI;

public class ChangeVendorTitle : MonoBehaviour {

    public void SetVendorName(string vname)
    {
        gameObject.GetComponent<UIWindowTitle>().SetTitle(vname);
        gameObject.GetComponent<UIWindowTitle>().OnValidate();
    }
}
